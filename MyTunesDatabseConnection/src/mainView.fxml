<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="240.0" prefWidth="737.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mainController">
   <children>
      <AnchorPane fx:id="Pane" prefHeight="284.0" prefWidth="737.0">
         <children>
            <Label fx:id="songLabel" alignment="CENTER" prefHeight="100.0" prefWidth="737.0" style="-fx-background-color: #ADD8E6;" text="Music Player">
               <font>
                  <Font size="58.0" />
               </font>
            </Label>
            <ProgressBar fx:id="songProgressBar" layoutY="100.0" prefHeight="16.0" prefWidth="737.0" progress="0.0" />
            <Button fx:id="playButton" layoutX="35.0" layoutY="156.0" mnemonicParsing="false" onAction="#playMedia" prefHeight="70.0" prefWidth="96.0" text="Play">
               <font>
                  <Font size="22.0" />
               </font>
            </Button>
            <Button fx:id="pauseButton" layoutX="141.0" layoutY="156.0" mnemonicParsing="false" onAction="#pauseMedia" prefHeight="70.0" prefWidth="96.0" text="Pause">
               <font>
                  <Font size="22.0" />
               </font>
            </Button>
            <Button fx:id="resetButton" layoutX="247.0" layoutY="156.0" mnemonicParsing="false" onAction="#resetMedia" prefHeight="70.0" prefWidth="96.0" text="Reset">
               <font>
                  <Font size="22.0" />
               </font>
            </Button>
            <Button fx:id="previousButton" layoutX="353.0" layoutY="156.0" mnemonicParsing="false" onAction="#previousMedia" prefHeight="70.0" prefWidth="96.0" text="Previous">
               <font>
                  <Font size="17.0" />
               </font>
            </Button>
            <Button fx:id="nextButton" layoutX="459.0" layoutY="156.0" mnemonicParsing="false" onAction="#nextMedia" prefHeight="70.0" prefWidth="96.0" text="Next">
               <font>
                  <Font size="22.0" />
               </font>
            </Button>
            <ComboBox fx:id="speedBox" layoutX="589.0" layoutY="170.0" onAction="#changeSpeed" prefHeight="42.0" prefWidth="96.0" promptText="Speed" />
            <Slider fx:id="volumeSlider" layoutX="567.0" layoutY="249.0" max="200.0" value="100.0" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
